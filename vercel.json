{
  "buildCommand": "chmod +x node_modules/.bin/vite && node_modules/.bin/vite build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/tools/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}